@model VirtualVendingMachine.Data.DTO.CoinsDto

@for (var i = 0; i < Model.Coins.Count; i++)
{
    <div class="row form-group">
        <button onclick="insertCoin(@Model.Coins[i].CurrencyId, '@Model.Coins[i].CoinName', @Model.Coins[i].Value)" class="btn btn-default">@Model.Coins[i].CoinName : <i class="fa fa-eur">@Model.Coins[i].Value</i></button>
    </div>

}
